<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    >
<ScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">



    <TextView
        android:id="@+id/women"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="WOMEN"
        android:textSize="25dp"
        android:textColor="@color/black"
        android:fontFamily="@font/tenor"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="20dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <TextView
        android:id="@+id/man"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MAN"
        android:textColor="@color/black"
        android:textSize="25dp"
        android:fontFamily="@font/tenor"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="150dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        />

    <TextView
        android:id="@+id/kids"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="KIDS"
        android:textColor="@color/black"
        android:textSize="25dp"
        android:fontFamily="@font/tenor"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="240dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <View
        android:id="@+id/viewline"
        android:layout_width="330dp"
        android:layout_height="0.3dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:background="@color/lightgrey"
        app:layout_constraintTop_toBottomOf="@id/women"
        app:layout_constraintLeft_toLeftOf="parent"
        />

    <TextView
        android:id="@+id/neww"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New"
        android:textColor="@color/black"
        android:textSize="25dp"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="20dp"
        android:fontFamily="@font/tenor"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/viewline"/>

    <ImageView
        android:id="@+id/newarrow"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="35dp"
        android:layout_marginRight="60dp"
        android:src="@drawable/ic_baseline_keyboard_arrow_down_24"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/kids"
        app:tint="#070707" />

    <TextView
        android:id="@+id/apparel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Apparel"
        android:textSize="25dp"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="20dp"
        android:fontFamily="@font/tenor"
        android:textColor="@color/black"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/neww"/>

    <ImageView
        android:id="@+id/apparelarrow"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="30dp"
        android:layout_marginRight="60dp"
        android:src="@drawable/ic_baseline_keyboard_arrow_down_24"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/newarrow"
        app:tint="#000000" />

    <TextView
        android:id="@+id/bag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bag"
        android:textColor="@color/black"
        android:textSize="25dp"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="20dp"
        android:fontFamily="@font/tenor"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/apparel"/>

    <ImageView
        android:id="@+id/bagarrow"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="30dp"
        android:layout_marginRight="60dp"
        android:backgroundTint="@color/black"
        android:src="@drawable/ic_baseline_keyboard_arrow_down_24"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/apparelarrow"
        app:tint="#000000" />

    <TextView
        android:id="@+id/shoes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Shoes"
        android:textColor="@color/black"
        android:textSize="25dp"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="20dp"
        android:fontFamily="@font/tenor"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/bag"/>

    <ImageView
        android:id="@+id/shoesarrow"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="30dp"
        android:layout_marginRight="60dp"

        android:src="@drawable/ic_baseline_keyboard_arrow_down_24"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/bagarrow"
        app:tint="#0E0E0E" />

    <TextView
        android:id="@+id/beauty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Beauty"
        android:textSize="25dp"
        android:textColor="@color/black"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="20dp"
        android:fontFamily="@font/tenor"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/shoes"/>

    <ImageView
        android:id="@+id/beautyarrow"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="30dp"
        android:layout_marginRight="60dp"
        android:src="@drawable/ic_baseline_keyboard_arrow_down_24"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/shoesarrow"
        app:tint="#0E0E0E" />
    <TextView
        android:id="@+id/accessories"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Accessories"
        android:textSize="25dp"
        android:textColor="@color/black"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="20dp"
        android:fontFamily="@font/tenor"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/beauty"/>

    <ImageView
        android:id="@+id/accessoriesarrow"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="30dp"
        android:layout_marginRight="60dp"
        android:outlineAmbientShadowColor="@color/black"
        android:src="@drawable/ic_baseline_keyboard_arrow_down_242"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/beautyarrow"
        app:tint="#050505" />

    <TextView
        android:id="@+id/mobile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="(786) 713-8616"
        android:textColor="@color/black"
        android:fontFamily="@font/tenor"
        android:textSize="25dp"
        android:drawableStart="@drawable/ic_baseline_call_24"
        android:layout_marginTop="60dp"
        android:drawablePadding="5dp"
        android:layout_marginLeft="20dp"
        app:layout_constraintTop_toBottomOf="@id/accessories"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <TextView
        android:id="@+id/location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Store locator"
        android:textColor="@color/black"
        android:fontFamily="@font/tenor"
        android:textSize="20dp"
        android:drawableStart="@drawable/ic_baseline_location_on_24"
        android:layout_marginTop="17dp"
        android:drawablePadding="5dp"
        android:layout_marginLeft="20dp"
        app:layout_constraintTop_toBottomOf="@id/mobile"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <ImageView
        android:id="@+id/divider7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/divider"
        android:layout_marginTop="30dp"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@id/location"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>

    <ImageView
        android:id="@+id/twitter"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:src="@drawable/twitter"
        app:layout_constraintTop_toBottomOf="@id/divider7"
        android:layout_marginTop="40dp"
        android:layout_marginLeft="130dp"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <ImageView
        android:id="@+id/instafollow"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:src="@drawable/instablack"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/divider7"
        android:layout_marginTop="40dp"/>

    <ImageView
        android:id="@+id/youtube"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:src="@drawable/youtube"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/divider7"
        android:layout_marginRight="130dp"
        android:layout_marginTop="38dp"/>

    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>